#include<stdio.h>
int main()
{
    float otpay;
    int hour,i=1;
    while(i<=10)
    {
        printf("enter no. of hour worked\n");
        scanf("%f",hour);
        if(hour>=40)
        otpay=(hour-40)*120;
        else
        otpay=0;
        printf("hours=%d overtime pay=Rs.%f\n,hour,otpay");
        i++;
    }
    return 0;
}
